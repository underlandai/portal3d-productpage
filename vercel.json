{
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "src/pages/api/send-email.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/_astro/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
